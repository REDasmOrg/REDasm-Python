<?xml version="1.0"?>
<typesystem package="REDasmPython">
  <primitive-type name="apilevel_t"/>
  <namespace-type name="REDasm">
    <object-type name="Context"/>
    <object-type name="Database"/>
    <object-type name="Object"/>
    <object-type name="Plugin"/>
    <object-type name="PluginDescriptor"/>

    <object-type name="LoadRequest"/>
    <object-type name="Loader">
      <modify-function signature="assembler() const" remove="all"/>
    </object-type>

    <object-type name="AssemblerRequest" copyable="yes"/>

    <object-type name="Assembler">
      <modify-function signature="setInstructionFlags(instruction_id_t, flag_t)" remove="all"/>
    </object-type>

    <object-type name="CapstoneAssembler"/>

    <object-type name="Printer"/>
    <object-type name="CapstonePrinter"/>
    <object-type name="Algorithm"/>
    <object-type name="Analyzer"/>

    <object-type name="Disassembler">
      <modify-function signature="getHexDump(address_t, const REDasm::Symbol**)" remove="all"/>
    </object-type>

  </namespace-type>
</typesystem>

