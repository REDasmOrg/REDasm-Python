<?xml version="1.0"?>
<typesystem package="REDasmPython">
  <namespace-type name="REDasm">
    <object-type name="Segment">
      <enum-type name="Type"/>
      <modify-field name="name_" remove="all"/>
    </object-type>
    <object-type name="Symbol">
      <enum-type name="Type"/>
      <enum-type name="Flags"/>
    </object-type>
    <object-type name="Operand">
      <enum-type name="Type"/>
      <enum-type name="Flags"/>
    </object-type>
    <object-type name="Instruction">
      <enum-type name="Type"/>
      <enum-type name="Flags"/>
      <modify-field name="targets" remove="all"/>
      <modify-field name="operandsstruct" remove="all"/>
      <modify-field name="puserdata" remove="all"/>
      <modify-field name="mnemonic_" remove="all"/>
      <modify-field name="name_" remove="all"/>
      <modify-field name="free" remove="all"/>
    </object-type>

    <object-type name="RegisterOperand"/>
    <object-type name="DisplacementOperand"/>

    <object-type name="BufferView">
      <modify-function signature="operator [](size_t) const" remove="all"/>
      <modify-function signature="operator [](size_t)" remove="all"/>
      <modify-function signature="operator +(size_t) const" remove="all"/>
      <modify-function signature="operator +=(size_t)" remove="all"/>
      <modify-function signature="operator ++()" remove="all"/>
      <modify-function signature="operator ++(int)" remove="all"/>
    </object-type>

    <object-type name="State"/>
    <object-type name="SymbolTable"/>
    <object-type name="ListingItem"/>
  </namespace-type>
</typesystem>
